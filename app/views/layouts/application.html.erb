<!DOCTYPE html>
<html>
<head>
  <title>GameReviewApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if logged_in %>
    <div id="logout">
      <%= button_to 'Logout', logout_path, :method => :delete %>
    </div>
  <% end %>
<%= yield %>

</body>
</html>
